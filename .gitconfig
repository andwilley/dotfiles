[user]
    email = andwilley@gmail.com
    name = Drew Willey
[push]
    default = simple
[core]
    editor = vim
    excludesfile = /home/rafiki/.gitignore_global
[diff]
    tool = default-difftool
[difftool "default-difftool"]
    cmd = code --wait --diff $LOCAL $REMOTE
[mergetool]
    keepBackup = false
    prompt = false
[merge]
    tool = vimdiff
    conflictstyle = diff3
[alias]
    st = status
    co = checkout
    com = checkout master
    cm = commit -m
    br = branch
    aa = add --all
